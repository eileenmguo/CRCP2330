/** Hello World program. */

class Main {
	function void main () {
		var int row, column;
		do Screen.setColor(true);
		let row = 16;
		while (row < 256) {
			let column = 16;
			while (column < 512) {
				do Screen.drawCircle(column, row, 4);
				let column = column + 16;
			}
			let row = row + 16;
		}
		return;
	}
}