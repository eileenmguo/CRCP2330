/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Or(a = a, b = b, out = notOut);
    Nand(a = a, b = b, out = andOut);
    And(a = notOut, b = andOut, out = out);
}